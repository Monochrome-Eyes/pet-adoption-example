@model midterm_project.Models.Pet

@{
	ViewData["Title"] = "All Pets";
}

<h1>Create a New Cat</h1>
<form asp-controller="Pet" asp-action="Create" method="post">
	<partial name="_PetInputForm" model="Model" />

	<div class="mt-3">
		<a class="btn btn-light mx-1" asp-controller="Pet" asp-action="List">Back</a>
		<input type="submit" class="btn btn-primary mx-1" value="Create" />
	</div>
</form>
